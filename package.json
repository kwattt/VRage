{
  "name": "@kwattt/vrage",
  "version": "0.0.2",
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@kwattt"
  },
  "exports": {
    ".": {
      "types": "./dist/server/index.d.ts",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "require": "./dist/client/*.js",
      "default": "./dist/client/*.js"
    },
    "./server/*": {
      "types": "./dist/server/*.d.ts",
      "require": "./dist/server/*.js",
      "default": "./dist/server/*.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "client/*": ["./dist/client/*"],
      "server/*": ["./dist/server/*"]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "build": "tsup"
  },
  "license": "MIT",
  "keywords": [
    "ragemp",
    "typescript",
    "vrage",
    "framework"
  ],
  "author": "kwattt",
  "devDependencies": {
    "@swc/core": "^1.9.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/underscore": "^1.13.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsc": "^2.0.4",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "yarn": "^1.22.22"
  },
  "dependencies": {
    "@ragempcommunity/types-client": "^2.1.8",
    "@ragempcommunity/types-server": "^2.1.8",
    "@types/pg": "^8.11.10",
    "bcryptjs": "^2.4.3",
    "kysely": "^0.27.4",
    "mysql2": "^3.11.4",
    "pg": "^8.13.1",
    "underscore": "^1.13.7"
  }
}